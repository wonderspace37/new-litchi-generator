{
    "version": 2,
    "functions": {
        "api/index.py": {
            "runtime": "python3.11",
            "memory": 512,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/generate",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production"
    }
}