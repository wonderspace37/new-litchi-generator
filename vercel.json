{
    "version": 2,
    "functions": {
        "api/index.py": {
            "memory": 512,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/generate",
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ]
}